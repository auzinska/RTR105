   46  nano gnuplot_examle_1.gp 
   47  #!/usr/local/bin/gnuplot -persist
   48  # set terminal pngcairo  transparent enhanced font "arial,10" fontscale 1.0 size 600, 400 
   49  # set output 'fillcrvs.7.png'
   50  set format x "" 
   51  set format y "" 
   52  set grid nopolar
   53  set grid xtics nomxtics ytics nomytics noztics nomztics nortics nomrtics  nox2tics nomx2tics noy2tics nomy2tics nocbtics nomcbtics
   54  set grid layerdefault   lt 0 linecolor 0 linewidth 0.500,  lt 0 linecolor 0 linewidth 0.500
   55  set object  1 rect from graph 0, 0 to graph 1, 1
   56  set object  1 behind clip lw 1.0  dashtype solid fc  rgb "#afffff"  fillstyle   solid 1.00 border lt -1
   57  unset parametric
   58  set title "world.dat plotted with filledcurves" 
   59  set xrange [ -180.000 : 180.000 ] noreverse nowriteback
   60  set x2range [ * : * ] noreverse writeback
   61  set yrange [ -70.0000 : 80.0000 ] noreverse nowriteback
   62  set y2range [ * : * ] noreverse writeback
   63  set zrange [ * : * ] noreverse writeback
   64  set cbrange [ * : * ] noreverse writeback
   65  set rrange [ * : * ] noreverse writeback
   66  set lmargin  1
   67  set colorbox vertical origin screen 0.9, 0.2 size screen 0.05, 0.6 front  noinvert bdefault
   68  NO_ANIMATION = 1
   69  ## Last datafile plotted: "world.dat"
   70  plot 'world.dat' with filledcurve notitle fs solid 1.0 lc rgb 'dark-goldenrod'
   71  nano gnuplot_examle_1.gp 
   72  # set terminal pngcairo  transparent enhanced font "arial,10" fontscale 1.0 size 600, 400 
   73  # set output 'fillcrvs.7.png'
   74  set format x ""
   75  set format y ""
   76  set grid nopolar
   77  set grid xtics nomxtics ytics nomytics noztics nomztics nortics nomrtics  nox2tics nomx2tics noy2tics nomy2tics nocbtics nomcbtics
   78  set grid layerdefault   lt 0 linecolor 0 linewidth 0.500,  lt 0 linecolor 0 linewidth 0.500
   79  set object  1 rect from graph 0, 0 to graph 1, 1
   80  set object  1 behind clip lw 1.0  dashtype solid fc  rgb "#afffff"  fillstyle   solid 1.00 border lt -1
   81  unset parametric
   82  set title "world.dat plotted with filledcurves"
   83  set xrange [ -180.000 : 180.000 ] noreverse nowriteback
