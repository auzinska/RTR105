   95  nano gnuplot_examle_1.gp 
   96  #!/usr/bin/gnuplot -persist
   97  # set terminal pngcairo  transparent enhanced font "arial,10" fontscale 1.0 size 600, 400 
   98  # set output 'pm3d.5.png'
   99  set border 4095 front lt black linewidth 1.000 dashtype solid
  100  unset parametric
  101  set view 50, 50, 1, 1
  102  set samples 25, 25
  103  set isosamples 20, 20
  104  unset surface
  105  set style data lines
  106  set xyplane relative 0
  107  set title "set pm3d at bstbst (funny combination, only for screen or postscript)"
  108  set xlabel "x"
  109  set xrange [ -15.0000 : 15.0000 ] noreverse nowriteback
  110  set x2range [ * : * ] noreverse writeback
  111  set ylabel "y"
  112  set yrange [ -15.0000 : 15.0000 ] noreverse nowriteback
  113  set y2range [ * : * ] noreverse writeback
  114  set zrange [ -0.250000 : 1.00000 ] noreverse nowriteback
  115  set cbrange [ * : * ] noreverse writeback
  116  set rrange [ * : * ] noreverse writeback
  117  set pm3d implicit at bstbst
  118  set colorbox vertical origin screen 0.9, 0.2 size screen 0.05, 0.6 front  noinvert bdefault
  119  NO_ANIMATION = 1
  120  splot sin(sqrt(x**2+y**2))/sqrt(x**2+y**2)
  121  nano gnuplot_examle_1.gp 
  122  # set terminal pngcairo  transparent enhanced font "arial,10" fontscale 1.0 size 600, 400 
  123  # set output 'histerror.1.png'
  124  set border 3 front lt black linewidth 1.000 dashtype solid
  125  set style fill   solid 1.00 border lt -1
  126  set key fixed right top vertical Right noreverse enhanced autotitle columnhead nobox
  127  set style histogram errorbars gap 2 lw 2title textcolor lt -1
  128  set offsets 0, 0, 2, 0
  129  unset parametric
  130  set datafile separator "        "
  131  set style data histograms
  132  set xtics border in scale 1,0.5 nomirror norotate  autojustify
  133  set xtics  norangelimit
  134  set xtics   ()
  135  set ytics border in scale 1,0.5 nomirror norotate  autojustify
  136  set cbtics border in scale 1,0.5 nomirror norotate  autojustify
  137  set title "Histogram with error bars"
  138  set xrange [ * : * ] noreverse writeback
  139  set x2range [ * : * ] noreverse writeback
  140  set yrange [ 0.00000 : * ] noreverse writeback noextend
  141  set y2range [ * : * ] noreverse writeback
  142  set zrange [ * : * ] noreverse writeback
  143  set cbrange [ * : * ] noreverse writeback
  144  set rrange [ * : * ] noreverse writeback
  145  set bmargin  5
  146  set colorbox vertical origin screen 0.9, 0.2 size screen 0.05, 0.6 front  noinvert bdefault
  147  NO_ANIMATION = 1
  148  ## Last datafile plotted: "world.dat"
  149  plot 'world.dat' using 2:3 fs solid 0.5 lw 2 ti 'A',      '' using 4:5:xticlabel(1) fs solid 0.0 lw 2 ti>
  150  nano fun.c++
  151  gcc fun.c++ -lm
  152  nano fun.c++
  153  gcc fun.c++ -lm
  154  ./a.out
  155  nano fun.c++
  156  exit\
